{% if datas|length > 0 %}
        
    {% for data in datas %}
        <div class="panel panel-default">
            <div class="panel-heading">
                <h4 class="panel-title">
                    <a role="button" data-toggle="collapse" href="#tag-row-{{ data['tag_id'] }}">
                        <i class="fa fa-tag"></i>
                        {{ data['tag'] }}
                        <span class="badge">
                            {{ data['objects']|length }}
                        </span>
                    </a>
                </h4>
            </div>
            <div id="tag-row-{{ data['tag_id'] }}" class="panel-collapse collapse">
                <div class="panel-body">
                    <ul class="list-unstyled">
                        {% for object in data['objects'] %}
                            <li>{{ object['name'] }}</li>
                        {% endfor %}
                    </ul>
                </div>
            </div>
        </div>
    {% endfor %}
{% else %}
    <div class="alert alert-warning">
        {{ 'no_tag'|trans({}, 'tag') }}
    </div>
{% endif %}